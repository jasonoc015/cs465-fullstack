<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>About - Travlr Getaways Web Template</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
			{{> header}}
			<div id="contents">
				<div class="box">
					<div>
						<div class="body">
							<h1>About</h1>
							{{#with aboutTop}}
							<h2>{{this.title}}</h2>
							{{{this.text}}}
							{{/with}}
							<hr>
							<div class="ads">
								{{#each rightAds}}
									<h2>{{this.title}}</h2>
									{{{this.description}}}
								{{/each}}
							</div>
								{{#each leftAds}}
									<h2>{{this.title}}</h2>
									{{{this.description}}}
								{{/each}}
						</div>
					</div>
				</div>
			</div>
		</div>
		{{> footer}}
	</div>
</body>
</html>